{
  "name": "karma-scss-preprocessor",
  "version": "1.0.3",
  "description": "Skeleton of a simple NPM Package with Babel and CI already configured.",
  "license": "ISC",
  "repository": "amercier/karma-scss-preprocessor",
  "author": "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "transpile": "rimraf dist && babel src -d dist",
    "prepublish": "npm run transpile",
    "doc": "rimraf esdoc && esdoc -c esdoc.json",
    "lint": "eslint .",
    "mocha": "mocha specs/lib --recursive --compilers js:babel-register",
    "start": "export NODE_ENV=development; npm run mocha -- --watch --growl",
    "test": "export NODE_ENV=test; npm run lint && rimraf coverage && nyc npm run mocha"
  },
  "main": "dist/lib/index.js",
  "keywords": [
    "package",
    "skeleton",
    "example",
    "node",
    "npm",
    "babel",
    "ci",
    "code",
    "codeclimate",
    "codecov",
    "coverage",
    "eslint",
    "gemnasium",
    "istanbul",
    "mocha"
  ],
  "dependencies": {
    "chalk": "^1.1.3"
  },
  "peerDependencies": {
    "node-sass": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.8.0",
    "babel-plugin-__coverage__": "^0.111111.1",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "codecov": "^1.0.1",
    "esdoc": "^0.4.7",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^3.0.0",
    "eslint-plugin-import": "^1.5.0",
    "install": "^0.7.3",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "npm": "^3.8.9",
    "nyc": "^6.4.0",
    "rimraf": "^2.5.2",
    "phantomjs-prebuilt": "^2.1.7",
    "sinon": "^1.17.4"
  },
  "nyc": {
    "exclude": "**",
    "reporter": [
      "lcovonly",
      "html",
      "text"
    ]
  }
}
